{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    }, {
      "files": "*.tsx",
      "options": {
        "parser": "typescript",
        "jsxBracketSameLine": true
      }
    }
  ]
 }